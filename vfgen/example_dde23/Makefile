# The PNG files are not generated by this Makefile.
# Do not delete them!

WEBFILES = MackeyGlass_dde23.m MackeyGlass_dde23_demo.m \
           SIRdelay_dde23.m 

VFGEN_EXAMPLE1_OUTPUT = MackeyGlass_dde23.m MackeyGlass_dde23_demo.m
VFGEN_EXAMPLE2_OUTPUT = SIRdelay_dde23.m

ALL = $(VFGEN_EXAMPLE1_OUTPUT) $(VFGEN_EXAMPLE2_OUTPUT)

all: $(ALL)

$(VFGEN_EXAMPLE1_OUTPUT): ../example_vf/MackeyGlass.vf
	vfgen dde23:demo=yes ../example_vf/MackeyGlass.vf

$(VFGEN_EXAMPLE2_OUTPUT): ../example_vf/SIRdelay.vf
	vfgen dde23 ../example_vf/SIRdelay.vf

clean:

purge:
	rm -f $(ALL)
