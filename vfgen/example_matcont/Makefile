# The PNG files are not generated by this Makefile.
# Do not delete them!

WEBFILES = bratu.m MorrisLecar.m

VFGEN_EXAMPLE1_OUTPUT = bratu.m
VFGEN_EXAMPLE2_OUTPUT = MorrisLecar.m

ALL = $(VFGEN_EXAMPLE1_OUTPUT) $(VFGEN_EXAMPLE2_OUTPUT)

all: $(ALL)

$(VFGEN_EXAMPLE1_OUTPUT): ../example_vf/bratu.vf
	vfgen matcont ../example_vf/bratu.vf

$(VFGEN_EXAMPLE2_OUTPUT): ../example_vf/MorrisLecar.vf
	vfgen matcont ../example_vf/MorrisLecar.vf

clean:

purge:
	rm -f $(ALL)
